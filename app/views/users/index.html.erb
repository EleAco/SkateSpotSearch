<h1 class="font-weight-bold text-center mb-5">ユーザー検索</h1>
  <div class="container">
    <% if @search_feeds.any? %>
      <div class="text-center">
        <%= render partial: "/shared/search_form/user_name_cont" %>
          <div class="row row-cols-1 row-cols-md-3" id="items">
              <%= render partial: 'users/users_card', collection: @search_feeds, as: "feed" %>
          </div>
        <%= link_to_next_page @search_feeds, 'つづきを見る', remote: true, id: 'more_link', class: 'btn btn-warning btn-lg' %>
      </div>
    </div>
    <% else %>
          <div class="text-center">
            <%= render'/shared/search_form/user_name_cont' %>
                <p>エラー</p>
    <% end %>
  </div>