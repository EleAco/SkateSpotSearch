<% @search_feeds.each do |result| %>
  <div class="col mb-4">
    <div class="card border-0">
      <div class="card-body">
        <%= link_to result.name, user_path(@q, :'id' => "#{result.id}") %>
        <%= image_tag result.image.url, class: "card-img-top border rounded-circle border-dark" %>
      </div>
    </div>
  </div>
<% end %>