<% @user.liked_maps.each do |map| %>
    <div class="min-card">
      <div class="min-image"><%= link_to image_tag(map.picture.url, size: "275x275"),map_path(map) %></div>
        <div class="min-info">
          <div class="min-title"><%= link_to map.title, map_path(map), class: "link_to_text_wh" %></div>
          <div class="min-detail"><%= link_to map.content, map_path(map), class: "link_to_text_wh" %></div>
        </div>
        <div class="min-social">
            <p class="font-xs">作成日&ensp;:&ensp;<%= map.created_at.strftime("%m/%d") %></p>
        </div>
    </div>
<% end %>